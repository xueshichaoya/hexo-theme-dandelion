<% if (index) { %>
    <% if (post.link) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url" title="<%= post.title %>"><%= post.title %></a>
    <% } else if (post.title) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>" title="<%= post.title %>"><%= post.title %></a>
    <% } else { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>" title="Untitled">Untitled</a>
    <% } %>
<% } else { %>
    <a class="<%= class_name %>" href="<%- url_for(post.path) %>" title="<%= post.title %>">
        <h1 class="<%= class_name %>" itemprop="name headline">
            <%= post.title %>
        </h1>
    </a>

    
<% } %>

